{
  "name": "leaderboardsgg",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "generate:api": "node ./scripts/generate-api.js",
    "lint:fix": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore . --fix",
    "lint:all": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint": "pnpm lint:all",
    "precommit": "pnpm lint:all && pnpm test && pnpm pretty:staged ",
    "prepare": "husky install",
    "prettier:check": "prettier --check --ignore-path .prettierignore .",
    "prettier:fix": "prettier --check --write --ignore-path .prettierignore .",
    "pretty": "pretty-quick",
    "pretty:staged": "pretty-quick --staged",
    "preview": "nuxt preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@nuxt/vite-builder": "3.0.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/i18n": "8.0.0-beta.3",
    "@nuxtjs/tailwindcss": "^6.1.1",
    "@types/dotenv-safe": "^8.1.2",
    "@types/eslint": "^8.4.8",
    "@types/node": "18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.41",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-vue": "^9.6.0",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "happy-dom": "^7.6.3",
    "husky": "^8.0.1",
    "nuxt": "3.0.0-rc.12",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "swagger-typescript-api": "^10.0.3",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.1.8",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.24.3",
    "vitest-fetch-mock": "^0.2.1",
    "vue": "^3.2.41",
    "webpack": "^5.74.0"
  }
