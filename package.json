{
  "name": "leaderboardsgg",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.13.1"
  },
  "engineStrict": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "generate:api": "node --use-system-ca ./scripts/generate-api.js",
    "lint:fix": "eslint --fix",
    "lint:all": "eslint .",
    "lint": "pnpm lint:all",
    "lint:prod": "set NODE_ENV=production&& eslint .",
    "lint:audit": "pnpm exec eslint-config-inspector",
    "precommit": "pnpm lint:prod && pnpm test && pnpm prettier:check ",
    "prepare": "husky",
    "prettier:check": "prettier --check --ignore-path .prettierignore .",
    "prettier:fix": "prettier --check --write --ignore-path .prettierignore .",
    "preview": "nuxt preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@vueuse/core": "^13.8.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.2.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/kit": "^4.0.3",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/vite-builder": "^4.0.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^10.0.6",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@opentelemetry/api": "^1.9.0",
    "@types/dotenv-safe": "^8.1.6",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.5.20",
    "@vue/test-utils": "^2.4.6",
    "dotenv-safe": "^9.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-typegen": "^2.3.0",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "nuxt": "^4.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "swagger-typescript-api": "13.2.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "unplugin-icons": "^22.2.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vitest-fetch-mock": "^0.4.5",
    "vue": "^3.5.20"
  }
}
