{
  "name": "leaderboardsgg",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "generate:api": "node ./scripts/generate-api.js",
    "lint:fix": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore . --fix",
    "lint:all": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint": "pnpm lint:all",
    "precommit": "pnpm lint:all && pnpm test && pnpm pretty:staged ",
    "prepare": "husky install",
    "prettier:check": "prettier --check --ignore-path .prettierignore .",
    "prettier:fix": "prettier --check --write --ignore-path .prettierignore .",
    "pretty": "pretty-quick",
    "pretty:staged": "pretty-quick --staged",
    "preview": "nuxt preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@nuxt/vite-builder": "3.0.0",
